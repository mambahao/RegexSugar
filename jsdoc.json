{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "excludeTags": ["author"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles":false
    },
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
