{
  "extends":"airbnb-base",
  "env": {
    "browser":true,
    "node":true,
    "mocha": true
  },
  "rules": {
    "radix":0,
    "no-nested-ternary":0,
    "new-cap":0,
    "wrap-iife": [2, "any"],
    "space-before-function-paren":[2, "never"],
    "key-spacing":0,
    "keyword-spacing":0,
    "vars-on-top":0,
    "func-names":0,
    "one-var-declaration-per-line":0,
    "consistent-return":0,
    "max-len":[1, 300, 2]
  }
}
